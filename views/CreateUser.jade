extends layout
block content
    body(ng-controller='WaterfallCtrl').
    <div style="margin-top: 70px;"> </div>
    <h1 style="margin-left:30px">Create User</h1>
    <form class="form-horizontal">
    <div class="form-group">
    <label class="control-label col-xs-3" ng-model="someArray[0].value" for="inputUserId">User Id</label>
    <div class="col-xs-2">
    <input type="text" class="form-control" ng-model="User.user_name" id="inputUserId" placeholder="User Name">
    </div>
    </div>
    <div class="form-group">
    <label class="control-label col-xs-3" for="inputStartDate">Email</label>
    <div class="col-xs-5">
    <input type='email' id="inputEmail"  ng-model='User.email' />
    </div>
    </div>
    <div class="form-group">
    <label class="control-label col-xs-3" for="inputEndDate">Password</label>
    <div class="col-xs-5">
    <input type='password' id="inputEndDate"  ng-model='User.password' />
    </div>
    </div>
    <div class="form-group">
    <div class="col-xs-offset-3 col-xs-9">
    <input type="submit" class="btn btn-primary" ng-click="CreateUser()" value="Create">
    <input type="reset" class="btn btn-default" value="Reset">
    </div>
</div>
div(ng-view)